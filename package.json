{
  "name": "spells",
  "version": "0.0.0",
  "description": "360 spells of 360 colors",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run clean && npm run build && npm run log_spells",
    "log_spells": "node -e \"var spells = require('./lib/index.js'); console.log(spells);\"",
    "clean": "rimraf lib",
    "build:lib": "babel src --out-dir lib && npm run build:lib:copy",
    "build:lib:copy": "cp src/*.json lib && cp -r src/spells lib",
    "build": "npm run build:lib",
    "prepublish": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xcatliu/spells.git"
  },
  "keywords": [
    "spells",
    "game",
    "colors",
    "magic"
  ],
  "author": "xcatliu <xcatliu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xcatliu/spells/issues"
  },
  "homepage": "https://github.com/xcatliu/spells#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "rimraf": "^2.5.1"
  }
}
